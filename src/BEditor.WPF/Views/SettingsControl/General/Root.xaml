<UserControl x:Class="BEditor.Views.SettingsControl.General.Root"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:BEditor.Views.SettingsControl.General"
             xmlns:data="clr-namespace:BEditor.Data;assembly=BEditor.Core"
             xmlns:resources="clr-namespace:BEditor.Properties;assembly=BEditor.Core"
             xmlns:md="http://materialdesigninxaml.net/winfx/xaml/themes"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <VirtualizingStackPanel>
        
        <md:ColorZone Content="{x:Static resources:Resources.General}" Style="{StaticResource SettingsColorZone}"/>
        
    </VirtualizingStackPanel>
</UserControl>
